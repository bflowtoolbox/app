  <eClassifiers xsi:type="ecore:EClass" name="Cluster" eSuperTypes="#//NamedElement #//All_Rel_3 #//RelationsObject">
    <eAnnotations source="gmf.node">
      <details key="label" value="name"/>
      <details key="label.icon" value="false"/>
      <details key="figure" value="polygon"/>
      <details key="polygon.x" value="0 10 10 10 90 90 90 100 100 0"/>
      <details key="polygon.y" value="0 0 50 0 0 50 0 0 50 50"/>
