      <details key="label.icon" value="false"/>
      <details key="figure" value="rectangle"/>
      <details key="size" value="100,50"/>
      <details key="border.color" value="0,0,0"/>
      <details key="color" value="22,22,22"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Relation1">
    <eAnnotations source="gmf.link">
      <details key="source" value="source"/>
      <details key="target" value="target"/>
      <details key="style" value="dash"/>
      <details key="target.decoration" value="arrow"/>
      <details key="color" value="0,0,0"/>
      <details key="source.constraint" value="self.oclIsKindOf(All_Rel_1) and self &lt;> oppositeEnd"/>
      <details key="target.constraint" value="(self.oclIsKindOf(All_Rel_1) and oppositeEnd.oclIsKindOf(All_Rel_1)) and not (self.oclIsTypeOf(Activity1)) and not (oppositeEnd.oclIsKindOf(All_Rel_1_EndPoint))"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//All_Rel_1"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//All_Rel_1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Relation2">
    <eAnnotations source="gmf.link">
      <details key="source" value="source"/>
      <details key="target" value="target"/>
      <details key="color" value="0,0,0"/>
      <details key="target.decoration" value="arrow"/>
      <details key="source.constraint" value="self.oclIsKindOf(All_Rel_1) and self &lt;> oppositeEnd"/>
      <details key="target.constraint" value="(self.oclIsTypeOf(Activity1) and oppositeEnd.oclIsTypeOf(Activity1)) or (self.oclIsTypeOf(Activity2) and oppositeEnd.oclIsTypeOf(Activity2)) or (self.oclIsTypeOf(Objective) and oppositeEnd.oclIsTypeOf(Objective)) or (self.oclIsTypeOf(Product) and oppositeEnd.oclIsTypeOf(Product))"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//All_Rel_1"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//All_Rel_1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Relation3">
    <eAnnotations source="gmf.link">
      <details key="source" value="source"/>
      <details key="target" value="target"/>
      <details key="color" value="0,0,0"/>
      <details key="source.constraint" value="self &lt;> oppositeEnd"/>
      <details key="target.constraint" value="(self.oclIsKindOf(All_Rel_1) and oppositeEnd.oclIsKindOf(RelationsObject)) or (self.oclIsKindOf(RelationsObject) and oppositeEnd.oclIsKindOf(All_Rel_1))"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//All_Rel_3"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//All_Rel_3"/>
  </eClassifiers>
</ecore:EPackage>
