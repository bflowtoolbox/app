  <eClassifiers xsi:type="ecore:EClass" name="All_Rel_3" abstract="true" eSuperTypes="#//All"/>
  <eClassifiers xsi:type="ecore:EClass" name="RelationsObject" abstract="true" eSuperTypes="#//All"/>
  <eClassifiers xsi:type="ecore:EClass" name="Activity1" eSuperTypes="#//NamedElement #//All_Rel_1 #//All_Rel_3">
    <eAnnotations source="gmf.node">
      <details key="label" value="name"/>
      <details key="label.icon" value="false"/>
      <details key="figure" value="polygon"/>
      <details key="polygon.x" value="0 0 85 100 85 0"/>
      <details key="polygon.y" value="0 50 50 25 0 0"/>
