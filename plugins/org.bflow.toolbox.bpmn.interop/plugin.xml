<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.gmf.runtime.common.ui.services.action.contributionItemProviders">
      <contributionItemProvider
            checkPluginLoaded="false"
            class="org.bflow.toolbox.bpmn.interop.BpmnInteropContributionItemProvider">
         <Priority
               name="Medium">
         </Priority>
         <partContribution
               class="org.bflow.toolbox.extensions.BflowDiagramEditor">
            <partAction
                  global="false"
                  id="org.bflow.toolbox.bpmn.interop.actions.convertToBpmn"
                  menubarPath="/diagramMenu/validationGroup">
            </partAction>
         </partContribution>
      </contributionItemProvider>
   </extension>

</plugin>
